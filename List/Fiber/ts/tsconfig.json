{
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	],
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"allowJs": true,
		"sourceMap": true,
		"lib": [
			"dom",
			"es2016"
		],
		"types": [
			"jasmine"
		],
		"typeRoots": [
			"node_modules/@types/**/*",
			"src/__typings__/**/*"
		],
		"allowSyntheticDefaultImports": true
	}
}
